// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Contact {
  id Int @id @default(autoincrement())
  name String?
  description String?
  phone String @unique
  email String
  address String?
  birthday String?
  anniversary String?
  relationship String?
  avatar_url String?
  slug String?
  facebook String?
  twitter String?
  instagram String?
  tiktok String?
  youtube String?
}

model Notes {
  id Int @id @default(autoincrement())
  image String?
  title String?
  slug String?
  note String?
  date String?
  tag String?
}