// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Contact {
  id Int @id @default(autoincrement())
  name String?
  description String?
  phone String @unique
  avatar_url String?
  slug String?
}
